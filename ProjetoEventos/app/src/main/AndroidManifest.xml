<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vinic.projetoeventos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".app.CadastroUsuarioActivity"
            android:parentActivityName=".app.LoginActivity" />
        <activity
            android:name=".app.CadastrarEventoActivity"
            android:parentActivityName=".app.MainActivity" />
        <activity
            android:name=".app.CadastrarAtividadeActivity"
            android:parentActivityName=".app.CadastrarEventoActivity" />
        <activity
            android:name=".app.EventoActivity"
            android:parentActivityName=".app.MainActivity" />
        <activity
            android:name=".app.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".app.InscricaoActivity"
            android:parentActivityName=".app.CadastrarEventoActivity" />
        <activity android:name=".app.AdicionarTagInteresseAcitivity"
            android:parentActivityName=".app.MainActivity"/>
    </application>

</manifest>